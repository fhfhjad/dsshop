
<view>
  <form  bindsubmit="formSubmit">
    <view class="zan-panel">
      
      <view class="zan-cell zan-field">
        <input class="zan-field__input zan-cell__bd" placeholder="车位号" type="text"  maxlength="20" name="parking_number" value="{{Address.information.recipient}}"></input>
      </view>
      
      <view class="zan-cell zan-field">
        <input class="zan-field__input zan-cell__bd" placeholder="车位位置" type="text" maxlength="100" name="parking_location" value="{{Address.information.telephone}}"></input>
      </view>
      
      <view class="zan-cell zan-field">
        <input class="zan-field__input zan-cell__bd" placeholder="标题" type="text"  maxlength="200" name="exchange_reason"  value="{{Address.information.address}}"></input>
      </view>
      
    </view>
    <view class="zan-btns">
        <button class="zan-btn zan-btn--primary" formType="submit">保存</button>
    </view>
  </form>
  
</view>

